interface=wlan0
domain=calicobox

expand-hosts
filterwin2k
read-ethers

dhcp-authoritative
dhcp-leasefile=/tmp/dhcp.leases

address=/#/192.168.112.1

dhcp-range=192.168.112.100,192.168.112.200,12h

dhcp-option=option:netmask,255.255.255.0
dhcp-option=option:router,0.0.0.0
dhcp-option=option:domain-name,calicobox
dhcp-option=option:dns-server,0.0.0.0
dhcp-option=option:ntp-server,0.0.0.0